#include <iostream>
#include <string>
using namespace std;
int main()
{
	string  a,b,c;
	
	cin>>a>>b>>c;
	
	
	if((a[0]==a[1] && a[1]==a[2]  && a[2]=='X') || (b[0]==b[1] && b[1]==b[2] && b[2]=='X' )|| (c[0]==c[1] &&  c[1]==c[2] && c[2]=='X') || (a[0]==b[0]  && b[0]==c[0] && c[0]=='X')|| (a[2]==b[2]  && b[2]==c[2]  &&  c[2]=='X') ||(a[1]=b[1] && b[1]==c[1] && c[1]=='X')||(a[0]==b[1]  && b[1]==c[2] && c[2]=='X') || (a[2]==b[1] && b[1]==c[0] && c[0]=='X'))
	cout<<"Win";
	else if((a[0]==a[1] && a[1]==a[2]  && a[2]=='O') || (b[0]==b[1] && b[1]==b[2] && b[2]=='O' )|| (c[0]==c[1] &&  c[1]==c[2] && c[2]=='O') || (a[0]==b[0]  && b[0]==c[0] && c[0]=='O')|| (a[2]==b[2] && b[2]==c[2]  && c[2]=='O') ||(a[1]=b[1] && b[1]==c[1] && c[1]=='O')||(a[0]==b[1]  && b[1]==c[2] && c[2]=='O') || (a[2]==b[1] && b[1]==c[0] &&  c[0]=='O'))
	cout<<"Lose";
	else
	cout<<"Draw";	
	
}
