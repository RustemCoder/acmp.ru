#include <iostream>
using namespace std;
int main()
{
     short int n;
    cin>>n;
    if(n>=1000){
        if((n%4==0 && n%100!=0)|| n%400==0)
    cout<<"12/09/"<<n;
    else
    cout<<"13/09/"<<n;
    }
else if(n>=100 && n<1000)
{
        if((n%4==0 && n%100!=0)|| n%400==0)
    cout<<"12/09/0"<<n;
    else
    cout<<"13/09/0"<<n;
}
else if(n<100)
{
     
        if((n%4==0 && n%100!=0)|| n%400==0)
    cout<<"12/09/000"<<n;
    else
    cout<<"13/09/000"<<n;
}
    return 0;
     
}
