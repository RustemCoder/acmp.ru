#include <iostream>
#include <algorithm>
using namespace std;
int main()


{
	
	
	int a,n,v,c,z,v1,c1,b;
	
	cin>>n;
	
	
	while(n>0)
	
	{
		n--;
		
		cin>>a;
		b=a+1;
		z=a-1;
		v=b/(100000)+ (b/10000)%10+(b/1000)%10;
		c=(b/100)%10+(b/10)%10+b%10;
		v1=z/(100000)+ (z/10000)%10+(z/1000)%10;
		c1=(z/100)%10+(z/10)%10+z%10;
		if(v==c || v1==c1)
		cout<<"Yes\n";
		else
		cout<<"No\n";
		
	}
	return 0;
}





