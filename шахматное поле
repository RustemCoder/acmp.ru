#include <iostream>
using namespace std;
int  main()
{
	
	int x1,y1,x2,y2;
	string a,b;
	cin>>x1>>y1>>x2>>y2;
	if((x1%2!=0 && y1%2!=0) ||(x1%2==0 && y1%2==0) )
	a="black";
	else
	a="white";
  if((x2%2!=0 && y2%2!=0) ||(x2%2==0 && y2%2==0) ) 
  b="black";
  else
  b="white";
  
  if(a==b)
  cout<<"YES";
  else
  cout<<"NO";

	return 0;


}
