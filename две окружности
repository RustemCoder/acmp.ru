#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	
	  int x1,y1,r1,x2,y2,r2,v;
	
	cin>>x1>>y1>>r1>>x2>>y2>>r2;
	
	v=sqrt(pow(x2-x1,2)+pow(y2-y1,2));
	
	if(v>(r1+r2))
	cout<<"NO";
	else if(v==0 && r1==r2)
	cout<<"YES";
	else if(v==0 && r1!=r2)
	cout<<"NO";
	else if(v<=(r1+r2))
	cout<<"YES";
	
	return 0;
}
